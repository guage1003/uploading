<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinguizi.mapper.SysLogMapper">
    <insert id="saveSysLog" parameterType="SysLog">
        insert into syslog values (null,#{username},#{operation},#{method},#{params},#{ip},#{createTime})
    </insert>
</mapper>